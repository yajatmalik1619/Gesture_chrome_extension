    "scroll_down": {
      "label": "Scroll Down",
      "type": "scroll",
      "repeatable": true,
      "direction": "down",
      "amount": 100
    },
    "scroll_stop": {
      "label": "Stop Scrolling",
      "type": "scroll",
      "repeatable": false,
      "direction": "stop"
