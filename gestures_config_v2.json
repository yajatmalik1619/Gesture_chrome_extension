{
  "settings": {
    "websocket_host": "localhost",
    "websocket_port": 8765,
    "max_num_hands": 2,
    "detection_confidence": 0.8,
    "tracking_confidence": 0.7,
    "model_complexity": 1,
    "gesture_buffer_size": 5,
    "dynamic_buffer_size": 5,
    "position_history_size": 25,
    "scroll_speed": 3,
    "two_hand_multiplier": {
      "enabled": true,
      "activation": "hold",
      "hold_duration_seconds": 0.5,
      "max_fingers": 5,
      "max_product": 25
    }
  },
  "actions": {
    "new_window": {
      "label": "Open New Window",
      "type": "keyboard",
      "repeatable": false,
      "shortcut": "ctrl+n",
      "shortcut_mac": "cmd+n"
    },
    "new_incognito_window": {
      "label": "Open Incognito Window",
      "type": "keyboard",
      "repeatable": false,
      "shortcut": "ctrl+shift+n",
      "shortcut_mac": "cmd+shift+n"
    },
    "tab_new": {
      "label": "New Tab",
      "type": "keyboard",
      "repeatable": false,
      "shortcut": "ctrl+t",
      "shortcut_mac": "cmd+t"
    },
    "tab_reopen": {
      "label": "Reopen Closed Tab",
      "type": "keyboard",
      "repeatable": false,
      "shortcut": "ctrl+shift+t",
      "shortcut_mac": "cmd+shift+t"
    },
    "tab_switch_right": {
      "label": "Switch Tab Right",
      "type": "keyboard",
      "repeatable": false,
      "modifier": "finger_count",
      "shortcut": "ctrl+tab",
      "shortcut_mac": "cmd+opt+right"
    },
    "tab_switch_left": {
      "label": "Switch Tab Left",
      "type": "keyboard",
